  <!-- Start of Parallex -->

  <section class="parallex py-5">
      <div class="container py-5 text-white text-center">
        <div class="row py-3">
          <div class="col-lg-9 mx-auto">
            <h1>Camping Products</h1>
            <p class="pt-3">“Walk away quietly in any direction and taste the freedom of the mountaineer. Camp out among the grasses and gentians of glacial meadows, in craggy garden nooks full of nature’s darlings.”</p>
            <h5 class="mb-5">— John Muir</h5>
              <button type="button" name="button" class="btn2 mr-1">Shop Now</button>
              <button type="button" name="button" class="btn3 mr-1">Get Deal</button>
          </div>
        </div>
      </div>
  </section>

